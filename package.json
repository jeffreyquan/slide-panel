{
  "name": "slide-panel",
  "version": "1.0.0",
  "description": "Slide Panel Web Component",
  "main": "dist/main.js",
  "scripts": {
    "start": "concurrently \"npm run scss:watch\"  \"npm run start:build\" \"npm run serve\"",
    "scss:watch": "node-sass -w -r src/style.scss -o ./src",
    "start:build": "rollup -w -c rollup.config.js",
    "serve": "live-server ./dist",
    "build": "rollup -c rollup.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "web-components",
    "slide-panel",
    "slide",
    "panel",
    "component"
  ],
  "author": "Jeffrey Quan",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffreyquan/slide-panel.git"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "concurrently": "^5.3.0",
    "live-server": "^1.2.1",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.6",
    "rollup": "^2.38.5",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  }
}
